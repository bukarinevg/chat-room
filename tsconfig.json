{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/app/*"],
      "@components/*": ["./src/app/components/*"],
      "@styles/*": ["./src/styles/*"],
      "@context/*": ["./src/context/*"],
      "@lib/*": ["./src/app/lib/*"],
      "@public/*" : ["./public/*"],
      // "@prisma": ["./prisma"],

    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/api/auth/[auth0]/route.js", "src/app/api/auth/[...nextauth].js"],
  "exclude": ["node_modules"]
}
